<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springbootdatabase.dao.DataDao" >

    <select id="getGu" resultType="com.springbootdatabase.model.Jsonreturn" parameterType="com.springbootdatabase.model.DataModel">
        select score,sang from gu WHERE bub=#{bub}
    </select>
    <select id="getSung" resultType="com.springbootdatabase.model.Jsonreturn" parameterType="com.springbootdatabase.model.DataModel">
        select score,sang from sung WHERE bub=#{bub}
    </select>
    <select id="getZip" resultType="com.springbootdatabase.model.Jsonreturn" parameterType="com.springbootdatabase.model.DataModel">
        select score,sang from zip WHERE bub=#{bub}
    </select>
    <select id="getGun" resultType="com.springbootdatabase.model.Jsonreturn" parameterType="com.springbootdatabase.model.DataModel">
        select score,sang from gun WHERE bub=#{bub}
    </select>
</mapper>